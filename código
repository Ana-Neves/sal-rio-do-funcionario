void main(){
  //Entrada de dados
  var horatrabalhada = 100;
  var salarioporhora = 5.50;
  var chapa = 25;
 
  
  
  
  // Processamento
 
  var salariopormes = horatrabalhada * salarioporhora;
  
  
  // Saída (show the results) 
  
  
  print('o número do funcionário é $chapa e o salário por mes do funcionário é igual á $salariopormes');
 
}
